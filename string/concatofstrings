#include<bits/stdc++.h>
using namespace std;
void concat(string s1,string s2)
{
    int l1=s1.length();
    int l2=s2.length();
    for(int i=0;i<l2;i++)
    {
       s1+=s2[i];
    }
    for(int i=0;i<s1.length();i++)
    cout<<s1[i];
}
int main()
{
    string s1,s2;
    cin>>s1>>s2;
    concat(s1,s2);
}
